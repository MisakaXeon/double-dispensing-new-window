<Window x:Class="dispensing.CustomControls.LoadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dispensing.CustomControls"
        mc:Ignorable="d"
        Title="LoadingWindow" Height="1920" Width="1080" WindowState="Normal" WindowStyle="None" Background="Black">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Styles/IconResources.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
            
        </ResourceDictionary>
    </Window.Resources>
    <Image Source="{StaticResource LoadingIcon}" Width="480px" Height="47px"></Image>
</Window>
